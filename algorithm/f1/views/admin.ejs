<html>

    <style>
        table, th, td {
          border: 1px solid black;
          border-collapse: collapse;
          color: white;
        }
        th, td {
          padding: 10px;
        }
        td a{
          color: yellow;
          text-decoration: none;
        }
        </style>
      <link rel="stylesheet" href="css/style2.css">
      <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-1BmE4kWBq78iYhFldvKuhfTAU6auU8tT94WrHftjDbrCEXSU1oBoqyl2QvZ6jIW3" crossorigin="anonymous">

    <body style="  background: #37474F; ">
       <div class="container" >
        <div>
            <h1 style="color: white;">Feedbacks</h1>
            <div class="message-box">
                <% if ( feedback.length==0 ) { %>
               There arent any feedbacks yet.
               <% } else { %>
             
           <table >
            <tr>
               <th>Feedback</th><th>User</th><th>Email</th><th>Date</th><th>Delete</th>
            </tr>
         
            <% for (var i = feedback.length-1; i >=0;  i--) { %>
             <tr>
               <td><%= feedback[i].feedback %></td>
               <td> <%= feedback[i].user %></td>
               <td> <%= feedback[i].email %></td>
               <% let date=feedback[i].updatedAt %>
               <% var day=date.getDate();   %>
               <% var month=date.getMonth()+1;  %>
               <% var year=date.getFullYear();  %>
               <% var h=date.getHours();  %>
               <% var m=date.getMinutes();  %>
               <% var fin=day+"-"+month+"-"+year+" "+h+":"+m%>
               <td><%=fin   %></td>
               <td><a href="/del/<%= feedback[i]._id %>">this</a></td> 
             </tr>    
            <% } %>
         </table>
         <% } %>
        </div><br>

        <div>
          <h1 style="color: white;">Algorithm Suggessions</h1>
          <div class="message-box">
              <% if ( algorithm.length==0 ) { %>
             There arent any algorithm suggessions yet.
             <% } else { %>
           
         <table >
          <tr>
             <th>algorithm</th><th>User</th><th>Date</th><th>Delete</th>
          </tr>
       
          <% for (var i = algorithm.length-1; i >=0;  i--) { %>
           <tr>
             <td><%= algorithm[i].algo %></td>
             <td> <%= algorithm[i].user %></td>
             <td> <%= algorithm[i].email %></td>
             <% let date=algorithm[i].updatedAt %>
             <% var day=date.getDate();   %>
             <% var month=date.getMonth()+1;  %>
             <% var year=date.getFullYear();  %>
             <% var h=date.getHours();  %>
             <% var m=date.getMinutes();  %>
             <% var fin=day+"-"+month+"-"+year+" "+h+":"+m%>
             <td><%=fin   %></td>
             <td><a href="/delal/<%= algorithm[i]._id %>">this</a></td> 
           </tr>    
          <% } %>
       </table>
       <% } %>
      </div><br>

        <form action="/logout" method="get">
            <input type="submit" class="btn btn-success" value="logout">
        </form>
        
          </div>
          </div>
    </body>
</html>